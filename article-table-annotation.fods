      <text:p><text:s/>FLASH: DUTERTE WINS PHILIPPINE PRESIDENTIAL ELECTION: PRELIMINARY RESULT</text:p>
     </table:table-cell>
     <table:table-cell office:value-type="string" calcext:value-type="string"><text:p/><text:p/><text:p>FLASH: DUTERTE WINS PHILIPPINE PRESIDENTIAL ELECTION: PRELIMINARY</text:p><text:p>RESULT</text:p><text:p/>
